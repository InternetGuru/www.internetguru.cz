<UrlHandler>

  <!--
    404; element notfound
    - atribut link     ... cesta

    např.:
    <notfound link="dymky/"/>
  -->
    <notfound link="tri"/>

  <!--
    PŘEPIS URL adresy; element rewrite
    - atribut match    ... přepisovaná část
    - obsah elementu   ... náhrada

    např.:
    <rewrite match="coop/"></rewrite>
  -->

  <!--
    ATRIBUTY elementu redir
    - link     ... celá cesta
    - parName  ... název parametru
    - parValue ... hodnota parametru

    CÍLOVÁ ADRESA (obsah elementu redir)
      /             ... domovská stránka
      bez cesty     ... zachová aktuální cestu, např. "?Admin"
      prázdná cesta ... smaže aktuální cestu, např. "/?Admin"
      bez parametru ... zachová aktuální parametry, např. "/"
      prázdný param ... smaže aktuální parametry, např. "?"
  -->

  <redir link="login">?login</redir>
  <redir link="Login">?login</redir>
  <redir link="Admin">/?Admin</redir>
  <redir link="admin">/?Admin</redir>
  <redir link="log">/?LogViewer=usr</redir>
  <redir link="ver">/?LogViewer=CHANGELOG</redir>
  <redir link="changelog">/?LogViewer=CHANGELOG</redir>
  <redir link="CHANGELOG">/?LogViewer=CHANGELOG</redir>
  <redir link="import">/?Convertor</redir>
  <redir link="subdom">/?SubdomManager</redir>

  <redir parName="ver" parValue="">?LogViewer=CHANGELOG</redir>
  <redir parName="changelog" parValue="">?LogViewer=CHANGELOG</redir>
  <redir parName="CHANGELOG" parValue="">?LogViewer=CHANGELOG</redir>
  <redir parName="log" parValue="">?LogViewer=usr</redir>

  <redir parName="PageSpeed" parValue="">?</redir>
  <redir parName="Debug" parValue="">?</redir>
  <redir parName="Cache" parValue="">?</redir>

  <redir parName="Login">?login</redir>
  <redir parName="admin">?Admin</redir>
  <redir parName="log">?LogViewer=usr</redir>
  <redir parName="ver">?LogViewer=CHANGELOG</redir>
  <redir parName="changelog">?LogViewer=CHANGELOG</redir>
  <redir parName="CHANGELOG">?LogViewer=CHANGELOG</redir>
  <redir parName="import">?Convertor</redir>
  <redir parName="subdom">?SubdomManager</redir>

</UrlHandler>

