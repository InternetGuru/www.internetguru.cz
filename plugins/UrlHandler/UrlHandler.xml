<?xml version="1.0" encoding="utf-8" ?>
<UrlHandler>

  <!--
    ATRIBUTY elementu var
    - link     ... cesta
    - parName  ... název parametru
    - parValue ... hodnota parametru

    CÍLOVÁ ADRESA (obsah elementu var)
      / nebo ""     ... domovská stránka
      bez cesty     ... zachová aktuální cestu, např. "?Admin"
      prázdná cesta ... smaže aktuální cestu, např. "/?Admin"
      bez parametru ... zachová aktuální parametry, např. "/"
      prázdný param ... smaže aktuální parametry, např. "/?"
  -->

  <var link="login">/</var>
  <var link="Admin">/?Admin</var>
  <var link="admin">/?Admin</var>
  <var link="log">/?LogViewer=log</var>
  <var link="ver">/?LogViewer=ver</var>
  <var link="import">/?Convertor</var>
  <var link="subdom">/?SubdomManager</var>

  <var parName="ver" parValue="">?LogViewer=ver</var>
  <var parName="log" parValue="">?LogViewer=log</var>

  <var parName="PageSpeed" parValue="on">?</var>

  <var parName="login">?</var>
  <var parName="admin">?Admin</var>
  <var parName="log">?LogViewer</var>
  <var parName="ver">?LogViewer</var>
  <var parName="import">?Convertor</var>
  <var parName="subdom">?SubdomManager</var>

</UrlHandler>
