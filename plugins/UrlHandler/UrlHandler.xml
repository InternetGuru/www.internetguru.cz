<UrlHandler>

  <!--
    Adresy běžící na https (prázné pro celý web)
  -->

  <https>?login</https>


  <!--
    ATRIBUTY elementu redir
    - link     ... cesta
    - parName  ... název parametru
    - parValue ... hodnota parametru

    CÍLOVÁ ADRESA (obsah elementu redir)
      / nebo ""     ... domovská stránka
      bez cesty     ... zachová aktuální cestu, např. "?Admin"
      prázdná cesta ... smaže aktuální cestu, např. "/?Admin"
      bez parametru ... zachová aktuální parametry, např. "/"
      prázdný param ... smaže aktuální parametry, např. "/?"
  -->

  <redir link="login">?login</redir>
  <redir link="Login">?login</redir>
  <redir link="Admin">/?Admin</redir>
  <redir link="admin">/?Admin</redir>
  <redir link="log">/?LogViewer=log</redir>
  <redir link="ver">/?LogViewer=ver</redir>
  <redir link="import">/?Convertor</redir>
  <redir link="subdom">/?SubdomManager</redir>

  <redir parName="ver" parValue="">?LogViewer=ver</redir>
  <redir parName="log" parValue="">?LogViewer=log</redir>

  <redir parName="PageSpeed" parValue="on">?</redir>
  <redir parName="Grunt" parValue="on">?</redir>

  <redir parName="Login">?login</redir>
  <redir parName="admin">?Admin</redir>
  <redir parName="log">?LogViewer</redir>
  <redir parName="ver">?LogViewer</redir>
  <redir parName="import">?Convertor</redir>
  <redir parName="subdom">?SubdomManager</redir>

</UrlHandler>

