<?xml version="1.0" encoding="utf-8" ?>
<UrlHandler>

  <!--
    ATTRIBUTES
    - link
    - parName
    - parValue

    DESTINATION (element content)
    - path part
     - if keeps current path if empty
     - / goes to root
    - parameter name
    - parameter value
     - keeps current parameter value if empty
  -->

  <var link="login">index</var>
  <var link="Admin">index?Admin</var>
  <var link="admin">index?Admin</var>
  <var link="log">index?LogViewer=log</var>
  <var link="ver">index?LogViewer=ver</var>
  <var link="import">index?Convertor</var>
  <var link="subdom">index?SubdomManager</var>

  <var parName="ver" parValue="">?LogViewer=ver</var>
  <var parName="log" parValue="">?LogViewer=log</var>

  <var parName="login">index</var>
  <var parName="admin">?Admin</var>
  <var parName="log">?LogViewer</var>
  <var parName="ver">?LogViewer</var>
  <var parName="import">?Convertor</var>
  <var parName="subdom">index?SubdomManager</var>

</UrlHandler>
