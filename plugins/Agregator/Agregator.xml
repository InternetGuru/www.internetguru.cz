<?xml version="1.0" encoding="utf-8" ?>
<Agregator>

  <parentid match=".*">abc</parentid>
  <prefixid match=".*">abc</prefixid>

  <!--

    HTML sestavy

    Dostupné proměnné:
      editlink, heading, link, author, authorid, resp, respid, ctime, mtime, short, desc, kw

    Atributy elementu html:
      id (povinné)
      class (nepovinné)
      sort | rsort (reverzní) s hodnotou dostupné proměnné (výchozí rsort="ctime")

  -->
  <html id="ul" wrapper="ul">
    <item><li><em class="noprint" var="editlink">n/a</em> <a title="Autor: %s" var="link@href author@title heading">n/a</a>, vytvořeno <em fn="inputvar-dateformat" var="ctime">n/a</em></li></item>
  </html>
  <html id="dl" wrapper="dl">
    <item>
      <dt><em class="noprint" var="editlink">n/a</em> <a title="Autor: %s" var="link@href author@title heading">n/a</a></dt>
      <dd>Poslední změna: <strong fn="inputvar-dateformat" var="mtime">n/a</strong></dd>
      <dd>Klíčová slova: <em var="kw">n/a</em></dd>
      <dd var="desc">n/a</dd>
    </item>
  </html>
  <html id="dd" sort="mtime">
    <item><dd><em class="noprint" var="editlink">n/a</em> <a var="link@href desc@title heading">n/a</a>, naposledy upraveno <span fn="inputvar-dateformat" var="mtime">n/a</span></dd></item>
  </html>

  <!--

    Obrázkové sestavy

    Dostupné proměnné:
      name, type, mtime, url, url-thumbs, url-preview, url-big, alt (pokud je definován)

    Atributy elementu html:
      id (povinné)
      class (nepovinné)
      sort | rsort (reverzní) s hodnotou dostupné proměnné (výchozí sort="name")

  -->
  <image id="gallery" wrapper="ul" class="gallery">
    <item>
      <li><a var="url-big@href"><object var="type@type url-thumbs@data alt@title name@title alt name"></object></a></li>
    </item>
  </image>

  <!--

    Zástupný text obrázku

    Element alt:
      atribut for (povinné) s hodnotou jména obrázku
      hodnota elementu (nepovinné) jako zástupný text obrázku

    Například:
      <alt for="sample320.jpg">Příklad zástupného textu obrázku</alt>

  -->

</Agregator>
