<?xml version="1.0" encoding="utf-8" ?>
<Agregator>

  <var modifyonly="modifyonly" id="creation">Autorem obsahu je <a href="" var="authorid@href author">n/a</a> ze dne <span var="ctime" fn="inputvar-dateformat">n/a</span></var>
  <var modifyonly="modifyonly" id="modified">Obsah dokumentu byl naposledy změněn dne <span fn="inputvar-dateformat" var="mtime">n/a</span></var>
  <var modifyonly="modifyonly" id="responsible">Za obsah zodpovídá <a href="" var="respid@href resp">n/a</a></var>
  <var modifyonly="modifyonly" id="part_created">Tato část byla vytvořena dne <span fn="inputvar-dateformat" var="ctime">n/a</span></var>
  <var modifyonly="modifyonly" id="part_modified">Tato část byla upravena dne <span fn="inputvar-dateformat" var="mtime">n/a</span></var>

  <!--

    Dostupné proměnné:
      editlink, heading, link, author, authorid, resp, respid, ctime, mtime, short, desc, kw

    Atributy elementu html:
      id (povinné)
      sort | rsort (reverzní) s hodnotou dostupné proměnné (výchozí rsort="ctime")

  -->
  <html id="ul" wrapper="ul">
    <item><li><em var="editlink">n/a</em> <a title="Autor: %s" var="link@href author@title heading">n/a</a>, vytvořeno <em fn="inputvar-dateformat" var="ctime">n/a</em></li></item>
  </html>
  <html id="dl" wrapper="dl">
    <item>
      <dt><em var="editlink">n/a</em> <a title="Autor: %s" var="link@href author@title heading">n/a</a></dt>
      <dd>Poslední změna: <strong fn="inputvar-dateformat" var="mtime">n/a</strong></dd>
      <dd>Klíčová slova: <em var="kw">n/a</em></dd>
      <dd var="desc">n/a</dd>
    </item>
  </html>
  <html id="dd" sort="mtime">
    <item><dd><em var="editlink">n/a</em> <a var="link@href desc@title heading">n/a</a>, naposledy upraveno <span fn="inputvar-dateformat" var="mtime">n/a</span></dd></item>
  </html>

