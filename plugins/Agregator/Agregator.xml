<?xml version="1.0" encoding="utf-8" ?>
<Agregator>
  <!--

    Dostupné proměnné:
      filepath, heading, link, author, authorid, resp, respid, ctime, mtime, short, desc, kw

    Atributy elementu html:
      id (povinné)
      sort | rsort (reverzní) s hodnotou dostupné proměnné

  -->
  <html id="html">
    <item><li><a title="Autor: %s" var="link@href author@title heading">n/a</a>, vytvořeno <em fn="inputvar-dateformatcs" var="ctime">n/a</em></li></item>
  </html>
  <html id="htmltop">
    <item>
      <dt><a title="Autor: %s" var="link@href author@title heading">n/a</a> (<em fn="inputvar-dateformatcs" var="ctime">n/a</em>)</dt>
      <dd var="kw">n/a</dd>
      <dd var="desc">n/a</dd>
    </item>
    <item><dt><a title="Autor: %s" var="link@href author@title heading">n/a</a> (<em fn="inputvar-dateformatcs" var="ctime">n/a</em>)</dt></item>
    <item since="4"/>
  </html>
  <html id="htmldetail">
    <item>
      <dt><a title="Autor: %s" var="link@href author@title heading">n/a</a> <em fn="strlen-cms-logged_user">[<a href="?Admin=%s" var="filepath@href">Editovat</a>]</em></dt>
      <dd>Poslední změna: <strong fn="inputvar-dateformatcs" var="mtime">n/a</strong></dd>
      <dd>Klíčová slova: <em var="kw">n/a</em></dd>
      <dd var="desc">n/a</dd>
    </item>
  </html>

  <!-- XSLT (todo)
