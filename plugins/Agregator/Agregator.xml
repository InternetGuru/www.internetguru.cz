<?xml version="1.0" encoding="utf-8" ?>
<Agregator>
  <!--

    Dostupné proměnné:
      editlink, heading, link, author, authorid, resp, respid, ctime, mtime, short, desc, kw

    Atributy elementu html:
      id (povinné)
      sort | rsort (reverzní) s hodnotou dostupné proměnné (výchozí rsort="ctime")

  -->
  <html id="li">
    <item><li><em var="editlink">n/a</em> <a title="Autor: %s" var="link@href author@title heading">n/a</a>, vytvořeno <em fn="inputvar-dateformat" var="ctime">n/a</em></li></item>
  </html>
  <html id="topdt">
    <item>
      <dt><em var="editlink">n/a</em> <a title="Autor: %s" var="link@href author@title heading">n/a</a> (<em fn="inputvar-dateformat" var="ctime">n/a</em>)</dt>
      <dd var="kw">n/a</dd>
      <dd var="desc">n/a</dd>
    </item>
    <item><dt><em var="editlink">n/a</em> <a title="Autor: %s" var="link@href author@title heading">n/a</a> (<em fn="inputvar-dateformat" var="ctime">n/a</em>)</dt></item>
    <item since="4"/>
  </html>
  <html id="dt">
    <item>
      <dt><em var="editlink">n/a</em> <a title="Autor: %s" var="link@href author@title heading">n/a</a></dt>
      <dd>Poslední změna: <strong fn="inputvar-dateformat" var="mtime">n/a</strong></dd>
      <dd>Klíčová slova: <em var="kw">n/a</em></dd>
      <dd var="desc">n/a</dd>
    </item>
  </html>
  <html id="dd" sort="mtime">
    <item><dd><em var="editlink">n/a</em> <a var="link@href desc@title heading">n/a</a>, naposledy upraveno <span fn="inputvar-dateformat" var="mtime">n/a</span></dd></item>
  </html>

  <!-- XSLT (todo)
  <xslt htmldir="article" file="article.xsl"/>
  <xslt htmldir="dokumenty" file="dokumenty.xsl"/>
  -->

  <!-- IMG (todo) -->

