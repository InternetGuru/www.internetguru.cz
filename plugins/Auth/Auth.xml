<?xml version="1.0" encoding="utf-8" ?>
<Auth>
  <!--
    Obsah elementu url
      [cesta][?název_parametru[=hodnota_parametru]]

    Hodnoty atributu access elementu url
      allow   ... pustí všechny
      deny    ... pustí jen přihlášené (výchozí, netřeba uvádět)
      denyall ... pustí jen vlastníka
  -->
  <url>?SubdomManager</url>
  <url>?Admin</url>
  <url>?LogViewer</url>
  <url>?Convertor</url>
  <url access="allow">?LogViewer=usr</url>
</Auth>
