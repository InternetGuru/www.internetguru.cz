<?xml version="1.0" encoding="utf-8" ?>
<Xhtml11>

  <!-- <themes readonly="readonly"/> -->
  <themes>
    <theme id="basic">
      <xslt>themes/lists.xsl</xslt>
      <xslt>themes/basicLayout.xsl</xslt>
      <xslt>themes/adminmenu.xsl</xslt>
      <favicon>themes/igcms.ico</favicon>
    </theme>
    <theme id="simple">
      <xslt>themes/lists.xsl</xslt>
      <xslt>themes/simpleLayout.xsl</xslt>
      <xslt>themes/adminmenu.xsl</xslt>
      <stylesheet media="screen">themes/simple.css</stylesheet>
      <favicon>themes/igcms.ico</favicon>
    </theme>
    <theme id="custom"/>
  </themes>

  <!-- available themes: basic or simple -->
  <var id="theme">simple</var>

  <!-- <xslt readonly="readonly"/> -->
  <stylesheet>themes/cms-default.css</stylesheet>
  <stylesheet media="print">themes/cms-print.css</stylesheet>
  <stylesheet>themes/toggleHidables.css</stylesheet>
  <!-- <stylesheet>themes/document.css</stylesheet> -->

  <jsFile readonly="readonly" />
  <jsFile append="body">themes/headingFragment.js</jsFile>
  <jsFile append="body">themes/toggleHidables.js</jsFile>

  <!-- <favicon>myFavicon.ico</favicon> -->

</Xhtml11>