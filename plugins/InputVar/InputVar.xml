<?xml version="1.0" encoding="utf-8"?>
<InputVar>

  <fn id="monthen" fn="replace">
    <data name="leden">January</data>
    <data name="únor">February</data>
    <data name="březen">March</data>
    <data name="duben">April</data>
    <data name="květen">May</data>
    <data name="červen">June</data>
    <data name="červenec">July</data>
    <data name="srpen">August</data>
    <data name="září">September</data>
    <data name="říjen">October</data>
    <data name="listopad">November</data>
    <data name="prosinec">December</data>
  </fn>
  <fn id="yearformat" fn="strftime" format="%Y"/>
  <fn id="tmpdateformat" fn="strftime" format="%B %e, %Y"/>
  <fn id="dateformat" fn="sequence">
    <call>tmpdateformat</call>
    <call>monthen</call>
  </fn>

  <var id="service_plain">Internet Guru, www.internetguru.cz</var>
  <var id="service">Powered by: Internet Guru</var>
  <var id="mtime">Last Modified: <span var="cms-mtime" fn="inputvar-dateformat">n/a</span></var>
  <var id="ielt7">
    <div class="ielte7" style="background: yellow; margin: 0; border: 0.3em solid red;">
      <dl>
        <dt>You are using outdated browser!</dt>
        <dd><a href="https://support.microsoft.com/cs-cz/help/17621/internet-explorer-downloads">Update</a> to newer version,</dd>
        <dd>install <a href="https://www.google.com/chrome/">different browser</a> or</dd>
        <dd>contact your system administrator.</dd>
      </dl>
    </div>
  </var>
  
  <var id="linkedin">fab fa-linkedin</var>
  <var id="skype">fab fa-skype</var>
  <var id="email">fas fa-envelope</var>
  <var id="facebook">fab fa-facebook</var>
  <var id="github">fab fa-github</var>
  <var id="google">fab fa-google-plus-square</var>
  <var id="phone">fas fa-phone-square</var>
  <var id="twitter">fab fa-twitter-square</var>
  <var id="map">fas fa-map</var>
  <var id="usd">fas fa-dollar-sign</var>
  <var id="cc">far fa-credit-card</var>
  
</InputVar>