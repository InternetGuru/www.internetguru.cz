<?xml version="1.0" encoding="utf-8" ?>
<InputVar>
  <var id="month_num" fn="date" format="%b"/>
  <var id="month_tr" fn="translate" name="$month_num">
    <cs name="01">ledna</cs>
    <cs name="02">února</cs>
    <cs name="03">března</cs>
    <cs name="04">dubna</cs>
    <cs name="05">května</cs>
    <cs name="06">června</cs>
    <cs name="07">července</cs>
    <cs name="08">srpna</cs>
    <cs name="09">září</cs>
    <cs name="10">října</cs>
    <cs name="11">listopadu</cs>
    <cs name="12">prosince</cs>
    <cs>n/a</cs>
  </var>
  <var id="today" fn="date" format="%e. $month_tr %Y"/>
