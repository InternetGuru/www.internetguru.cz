<?xml version="1.0" encoding="utf-8" ?>
<InputVar>

  <fn id="monthcs" fn="replace">
    <data name="leden">ledna</data>
    <data name="únor">února</data>
    <data name="březen">března</data>
    <data name="duben">dubna</data>
    <data name="květen">května</data>
    <data name="červen">června</data>
    <data name="červenec">července</data>
    <data name="srpen">srpna</data>
    <!-- <data name="září">září</data> -->
    <data name="říjen">října</data>
    <data name="listopad">listopadu</data>
    <data name="prosinec">prosince</data>
  </fn>
  <fn id="dateformat" fn="strftime" format="%e. %B %Y"/>
  <fn id="dateformatcs" fn="sequence">
    <call fn="dateformat"/>
    <call fn="monthcs"/>
  </fn>

  <var id="cyear" fn="strftime" format="%Y" var="cms-ctime"/>
  <var id="year" fn="strftime" format="%Y">now</var>
  <var id="today" fn="dateformatcs">now</var>
  <var id="myctime" fn="dateformatcs" var="cms-ctime"/>
  <var id="mymtime" fn="dateformatcs" var="cms-mtime"/>
  <var id="linkctime" fn="dateformatcs" var="contentlink-ctime"/>
  <var id="linkmtime" fn="dateformatcs" var="contentlink-mtime"/>

  <var id="creation">Autorem obsahu je <a var="contentlink-author contentlink-authorid@href">n/a</a> ze dne <em var="inputvar-linkctime">n/a</em></var>
  <var id="edit">[<a href="?Admin=%s" var="agregator-filepath@href">Upravit obsah</a>]</var>
  <var id="resp">Za obsah této stránky zodpovídá <a var="contentlink-resp contentlink-respid@href">n/a</a>.</var>
  <var id="modified">Obsah této stránky byl naposledy upraven dne <em var="inputvar-linkmtime">n/a</em>.</var>

</InputVar>