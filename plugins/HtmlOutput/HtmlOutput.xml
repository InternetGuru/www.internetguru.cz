<?xml version="1.0" encoding="utf-8" ?>
<HtmlOutput>

  <!-- <themes readonly="readonly"/> -->
  <themes>
    <theme id="basic">
      <xslt>themes/lists.xsl</xslt>
      <xslt>themes/basicLayout.xsl</xslt>
      <xslt>themes/footer.xsl</xslt>
      <xslt>themes/adminmenu.xsl</xslt>
      <stylesheet media="screen">themes/basic.css</stylesheet>
    </theme>
    <theme id="simple">
      <xslt>themes/lists.xsl</xslt>
      <xslt>themes/simpleLayout.xsl</xslt>
      <xslt>themes/footer.xsl</xslt>
      <xslt>themes/adminmenu.xsl</xslt>
      <stylesheet media="screen">themes/simple.css</stylesheet>
    </theme>
    <theme id="custom"/>
  </themes>

  <!-- available themes: basic or simple -->
  <var id="theme">simple</var>

  <!-- Default stylesheets -->
  <stylesheet>lib/opensans/opensans.css</stylesheet>
  <stylesheet>lib/flaticon/flaticon.css</stylesheet>
  <stylesheet>themes/cms-default.css</stylesheet>
  <stylesheet media="print">themes/cms-print.css</stylesheet>
  <!-- <stylesheet>themes/document.css</stylesheet> -->

  <!-- Default scripts -->
  <jsFile append="body">themes/syntaxhighlightjs.js</jsFile>
  <jsFile append="body">themes/fragments.js</jsFile>
  <jsFile append="body" priority="5">themes/toc.js</jsFile>
  <jsFile append="body" priority="5">themes/tocinit.js</jsFile>
  <jsFile append="body" priority="5">themes/selectable.js</jsFile>
  <jsFile append="body" priority="5">themes/selectableinit.js</jsFile>
  <jsFile append="body">themes/hideable.js</jsFile>
  <jsFile append="body">themes/eventable.js</jsFile>
  <jsFile append="body">themes/addressable.js</jsFile>
  <jsFile append="body">themes/addressableinit.js</jsFile>
  <jsFile append="body">themes/editable.js</jsFile>
  <jsFile append="body">themes/editableinit.js</jsFile>
  <jsFile append="body">themes/scrolltop.js</jsFile>
  <jsFile append="body">themes/scrolltopinit.js</jsFile>
  <jsFile append="body">themes/completable.js</jsFile>

  <!-- Browser compatibility -->
  <jsFile>themes/modernizer.js</jsFile>
  <jsFile if="lt IE 9">themes/ie9.js</jsFile>

  <!-- <xslt readonly="readonly"/> -->
  <xslt>themes/cms-messages.xsl</xslt>

  <!-- <favicon>themes/myFavicon.ico</favicon> -->

  <var id="robots" modifyonly="modifyonly">all</var>

  <!-- Favicons -->
  <var id="error" modifyonly="modifyonly">data:image/png,%89PNG%0D%0A%1A%0A%00%00%00%0DIHDR%00%00%00%10%00%00%00%10%08%02%00%00%00%90%91h6%00%00%00%19IDAT(%91c%BCd%AB%C2%40%0A%60%22I%F5%A8%86Q%0DCJ%03%00%DE%B5%01S%07%88%8FG%00%00%00%00IEND%AEB%60%82</var>
  <var id="warning" modifyonly="modifyonly">data:image/png;base64,</var>
  <var id="success" modifyonly="modifyonly">data:image/png,%89PNG%0D%0A%1A%0A%00%00%00%0DIHDR%00%00%00%10%00%00%00%10%08%02%00%00%00%90%91h6%00%00%00%19IDAT(%91c%0C%DD%10%C5%40%0A%60%22I%F5%A8%86Q%0DCJ%03%00dy%01%7F%0C%9F0%7D%00%00%00%00IEND%AEB%60%82</var>
  <var id="info" modifyonly="modifyonly">data:image/png;base64,</var>

</HtmlOutput>
