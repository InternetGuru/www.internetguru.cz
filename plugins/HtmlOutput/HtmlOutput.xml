<?xml version="1.0" encoding="utf-8" ?>
<HtmlOutput>

  <!-- Selected theme -->
  <var id="theme">default</var>

  <themes>
    <!-- Default theme -->
    <theme id="default">
      <!--
        Fonts
      -->
      <stylesheet>lib/roboto/roboto.css</stylesheet>
      <!--<stylesheet>lib/opensans/opensans.css</stylesheet>-->
      <!--<stylesheet if-xpath="//*[contains(@class, 'fa')]">lib/font-awesome/css/font-awesome.css</stylesheet>-->
      <!--
        Styles
      -->
      <stylesheet>themes/default/default.css</stylesheet>
      <stylesheet>themes/default/reset.css</stylesheet>
      <stylesheet media="screen" if-xpath="//div[@id='messages']">themes/default/messages.css</stylesheet>
      <stylesheet>themes/default/form.css</stylesheet>
      <stylesheet>themes/default/columns.css</stylesheet>
      <stylesheet>themes/default/breadcrumb.css</stylesheet>
      <stylesheet>themes/default/footer.css</stylesheet>
      <stylesheet>themes/default/all.css</stylesheet>
      <stylesheet media="screen">themes/default/screen.css</stylesheet>
      <stylesheet media="print">themes/default/print.css</stylesheet>
      <stylesheet if-xpath="/body[contains(@class, 'doc')]">themes/default/document.css</stylesheet>
      <!--
        Transformations
      -->
      <xslt>themes/default/lists.xsl</xslt>
      <xslt>themes/default/layout.xsl</xslt>
      <!--<xslt>themes/default/h1desc.xsl</xslt>-->
      <!--<xslt>themes/default/hdesc.xsl</xslt>-->
      <!--<xslt>themes/default/part.xsl</xslt>-->
      <xslt>themes/default/footer.xsl</xslt>
      <xslt>themes/default/ielte7.xsl</xslt>
      <xslt>themes/default/adminmenu.xsl</xslt>
      <xslt>themes/default/messages.xsl</xslt>
    </theme>
    <!-- Empty theme -->
    <theme id="custom"/>
    <!-- Obsolete theme -->
    <theme id="basic">
      <stylesheet>themes/cms-default.css</stylesheet>
      <stylesheet media="print">themes/cms-print.css</stylesheet>
      <stylesheet>lib/opensans/opensans.css</stylesheet>
      <stylesheet>lib/flaticon/flaticon.css</stylesheet>
      <xslt>themes/lists.xsl</xslt>
      <xslt>themes/basicLayout.xsl</xslt>
      <xslt>themes/footer.xsl</xslt>
      <xslt>themes/ielte7.xsl</xslt>
      <xslt>themes/adminmenu.xsl</xslt>
      <xslt>themes/cms-messages.xsl</xslt>
      <stylesheet media="screen">themes/basic.css</stylesheet>
    </theme>
    <!-- Obsolete theme -->
    <theme id="simple">
      <stylesheet>themes/cms-default.css</stylesheet>
      <stylesheet media="print">themes/cms-print.css</stylesheet>
      <stylesheet>lib/opensans/opensans.css</stylesheet>
      <stylesheet>lib/flaticon/flaticon.css</stylesheet>
      <xslt>themes/lists.xsl</xslt>
      <xslt>themes/simpleLayout.xsl</xslt>
      <xslt>themes/footer.xsl</xslt>
      <xslt>themes/ielte7.xsl</xslt>
      <xslt>themes/adminmenu.xsl</xslt>
      <xslt>themes/cms-messages.xsl</xslt>
      <stylesheet media="screen">themes/simple.css</stylesheet>
    </theme>
  </themes>

  <!-- IGCMS global object -->
  <jsFile append="head">themes/igcms.js</jsFile>

  <!-- Browser compatibility -->
  <jsFile>themes/modernizer.js</jsFile>
  <jsFile if="lte IE 8" priority="100">themes/events.js</jsFile>
  <jsFile if="lte IE 8" priority="100">themes/indexof.js</jsFile>
  <jsFile if="lte IE 8" priority="100">themes/textcontent.js</jsFile>
  <jsFile if="lte IE 9" priority="100">themes/classlist.js</jsFile>
  <jsFile if="lt IE 9" priority="100">themes/ie9.js</jsFile>
  <jsFile append="body">themes/iefixes.js</jsFile>
  <jsFile append="body">themes/browserdetect.js</jsFile>

  <!-- Default scripts -->
  <jsFile append="body">themes/highlightable.js</jsFile>
  <jsFile append="body">themes/fragmentable.js</jsFile>
  <jsFile append="body">themes/fragmentableinit.js</jsFile>
  <jsFile append="body">themes/scrolltopable.js</jsFile>
  <jsFile append="body">themes/scrolltopableinit.js</jsFile>
  <jsFile append="body" if-xpath="//*[contains(@class, 'hideable') or contains(@class, 'tocable')]" priority="20">themes/hideable.js</jsFile>
  <jsFile append="body" if-xpath="//*[contains(@class, 'hideable') or contains(@class, 'tocable')]" priority="20">themes/hideableinit.js</jsFile>
  <jsFile append="body" if-xpath="//*[contains(@class, 'tocable')]" priority="70">themes/tocable.js</jsFile>
  <jsFile append="body" if-xpath="//*[contains(@class, 'tocable')]" priority="70">themes/tocableinit.js</jsFile>
  <jsFile append="body" if-xpath="//code[not(contains(@class, 'nohighlight'))]">themes/syntaxhighlightjs.js</jsFile>
  <jsFile append="body" if-xpath="//*[contains(@class, 'filterable')]" priority="70">themes/filterable.js</jsFile>
  <jsFile append="body" if-xpath="//*[contains(@class, 'filterable')]" priority="70">themes/filterableinit.js</jsFile>
  <jsFile append="body" if-xpath="//*[contains(@class, 'selectable')]" priority="70">themes/selectable.js</jsFile>
  <jsFile append="body" if-xpath="//*[contains(@class, 'selectable')]" priority="70">themes/selectableinit.js</jsFile>
  <jsFile append="body" if-xpath="//*[contains(@class, 'eventable')]">themes/eventable.js</jsFile>
  <jsFile append="body" if-xpath="//*[contains(@class, 'eventable')]">themes/eventableinit.js</jsFile>
  <jsFile append="body" if-xpath="//*[contains(@class, 'addressable')]">themes/addressable.js</jsFile>
  <jsFile append="body" if-xpath="//*[contains(@class, 'addressable')]">themes/addressableinit.js</jsFile>
  <jsFile append="body" if-xpath="//*[contains(@class, 'editable')]">themes/editable.js</jsFile>
  <jsFile append="body" if-xpath="//*[contains(@class, 'editable')]">themes/editableinit.js</jsFile>
  <jsFile append="body" if-xpath="//select[contains(@class, 'completable')]">themes/completable.js</jsFile>

  <!-- <favicon>themes/myFavicon.ico</favicon> -->

  <!-- robots.txt and meta robots -->
  <robots readonly="readonly" meta="noindex, nofollow">User-agent: *
Disallow: /</robots>
  <robots domain="*.internetguru.cz" readonly="readonly" meta="noindex, nofollow">User-agent: *
Disallow: /</robots>
  <robots domain="www.*.*" readonly="readonly" meta="all">User-agent: *
Disallow:</robots>
  <robots domain="*.localhost.cz" readonly="readonly" meta="noindex, nofollow">User-agent: *
Disallow: /</robots>

  <!-- Favicons -->
  <var id="error" modifyonly="modifyonly">data:image/png,%89PNG%0D%0A%1A%0A%00%00%00%0DIHDR%00%00%00%10%00%00%00%10%08%02%00%00%00%90%91h6%00%00%00%19IDAT(%91c%BCd%AB%C2%40%0A%60%22I%F5%A8%86Q%0DCJ%03%00%DE%B5%01S%07%88%8FG%00%00%00%00IEND%AEB%60%82</var>
  <var id="warning" modifyonly="modifyonly">data:image/png;base64,</var>
  <var id="success" modifyonly="modifyonly">data:image/png,%89PNG%0D%0A%1A%0A%00%00%00%0DIHDR%00%00%00%10%00%00%00%10%08%02%00%00%00%90%91h6%00%00%00%19IDAT(%91c%0C%DD%10%C5%40%0A%60%22I%F5%A8%86Q%0DCJ%03%00dy%01%7F%0C%9F0%7D%00%00%00%00IEND%AEB%60%82</var>
  <var id="notice" modifyonly="modifyonly">data:image/png;base64,</var>

</HtmlOutput>
